{
  "name": "@whisker/schemas",
  "version": "1.0.0",
  "description": "JSON Schema definitions for The Whisker Shogunate entities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
